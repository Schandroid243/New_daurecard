@model Daurecard.Models.Subscribe


<div id="main" class="container">

    <div>
        <form id="subscribeForm" method="post" enctype="multipart/form-data" style="width:80%;">
            <hr />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group row">
                <label asp-for="ProductId" class="col-md-2"></label>
                <div class="col-md-10">
                    <input id="txtNom" asp-for="ProductId" class="form-control" />
                    <span asp-validation-for="ProductId" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group row" style="margin-top: 5px;">
                <label asp-for="LicenceKey" class="col-md-2"></label>
                <div class="col-md-10">
                    <input id="txtPrenom" asp-for="LicenceKey" class="form-control" />
                    <span asp-validation-for="LicenceKey" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group" style="margin-top: 5px;">
                <div class="col-md-10 offset-md-2">
                    <input id="btnSubmit" value="Enregistrer" type="submit" style="width: 120px;" class="btn btn-success" />
                </div>
            </div>
        </form>
    </div>
</div>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}